{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'rent-a-bike-nj/*'\" \"npm run rent-a-bike-nj\"",
    "client": "cd .. cd rent-a-bike-nj && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd .. cd rent-a-bike-nj && npm install",
    "build": "cd .. cd rent-a-bike-nj && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "create-react-app": "^4.0.3",
    "express": "^4.17.1",
    "mongoose": "^5.12.2",
    "morgan": "^1.10.0",
    "path": "^0.12.7"
  },
